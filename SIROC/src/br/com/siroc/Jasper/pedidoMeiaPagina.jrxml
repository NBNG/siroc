	<queryString>
		<![CDATA[select clientes.cli_nome, to_char(produtos.pro_peso*item_quantidade,'09D90') || 'Kg' as peso_total,
clientes.cli_endereco ||', ' ||clientes.cli_bairro || '. ' || clientes.cli_cidade || '-' || clientes.cli_estado as endereco,clientes.cli_telefone,
clientes.cli_cep,pedidos.ped_id, clientes.cli_cnpj_cpf, clientes.cli_inscricao_est,to_char(((select sum (itens.item_valor * itens.item_quantidade)
from itens inner join pedidos on itens.fk_pedido = pedidos.ped_id where pedidos.ped_id = 19)*fornecedores.for_frete)/100 ,'R$999G990D99') as frete,
produtos.pro_id, itens.item_quantidade, produtos.pro_nome || ' - ' || to_char(produtos.pro_peso,'09D90')|| ' Kg' as produto,
to_char(itens.item_valor,'R$999G990D99') as item_valor, to_char((itens.item_valor * itens.item_quantidade) ,'R$999G990D99') as total_parcial,
to_char((select sum((itens.item_valor - produtos.pro_saida)*itens.item_quantidade) from itens inner join pedidos on itens.fk_pedido = pedidos.ped_id
inner join produtos on itens.fk_produto = produtos.pro_id where pedidos.ped_id = 19),'L9G999G90D99')  as lucro, pedidos.ped_pagamento, pedidos.ped_pedido,
pedidos.ped_status, to_char(pedidos.ped_data,'dd/mm/yyyy') as data, to_char((select sum (itens.item_valor * itens.item_quantidade)
from itens inner join pedidos on itens.fk_pedido = pedidos.ped_id where pedidos.ped_id = 19),'R$999G990D99')  as total,to_char(pedidos.ped_vencimento,'dd/mm/yyyy') as vencimento,pedidos.ped_obs from pedidos
inner join clientes on clientes.cli_id = pedidos.fk_cliente inner join itens on pedidos.ped_id = itens.fk_pedido inner join produtos
on produtos.pro_id = itens.fk_produto  inner join fornecedores on produtos.fk_fornecedor = fornecedores.for_id
group by produtos.pro_peso,pedidos.ped_vencimento,pedidos.ped_obs,produtos.pro_nome,pedidos.ped_id,clientes.cli_nome,clientes.cli_endereco,clientes.cli_bairro,clientes.cli_cidade,clientes.cli_estado,clientes.cli_telefone,
clientes.cli_cep,clientes.cli_cnpj_cpf,clientes.cli_inscricao_est,fornecedores.for_frete,produtos.pro_id,itens.item_quantidade,itens.item_valor,
pedidos.ped_pagamento, pedidos.ped_pedido,pedidos.ped_status,pedidos.ped_data ORDER BY ped_id]]>
	</queryString>
