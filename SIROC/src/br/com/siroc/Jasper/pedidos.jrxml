	<queryString language="SQL">
		<![CDATA[select clientes.cli_nome,clientes.cli_endereco ||', ' ||clientes.cli_bairro || '. ' || clientes.cli_cidade || '-' || clientes.cli_estado as endereco,clientes.cli_telefone, clientes.cli_cep,clientes.cli_cnpj_cpf,
  clientes.cli_inscricao_est,to_char(((select sum(items.item_valor * items.item_quantidade) from items inner join pedidos on items.fk_pedido =
pedidos.ped_id inner join clientes on pedidos.fk_cliente = clientes.cli_id where clientes.cli_id = 2)*clientes.cli_frete)/100, 'R$99G999D99') as cli_frete,to_char(produtos.pro_saida, 'R$99G999D99') as pro_saida,
  pro_id,items.item_quantidade,produtos.pro_nome || ' Peso: ' || produtos.pro_peso as produto,  to_char(items.item_valor, 'R$99G999D99') as item_valor,
   to_char((items.item_valor * items.item_quantidade), 'R$99G999D99') as total_parcial, to_char((select sum((items.item_valor - produtos.pro_saida) *
  items.item_quantidade) from items inner join produtos on items.fk_produto = produtos.pro_id inner join pedidos
  on items.fk_pedido = pedidos.ped_id inner join clientes on pedidos.fk_cliente = clientes.cli_id
  where clientes.cli_id = 2), 'R$99G999D99') as saldo,pedidos.ped_pagamento,pedidos.ped_pedido,pedidos.ped_status,
  to_char((select sum(items.item_valor * items.item_quantidade) from items inner join pedidos on items.fk_pedido = pedidos.ped_id
  where pedidos.fk_cliente = 2), 'R$99G999D99') as total from pedidos inner join clientes on
  clientes.cli_id = pedidos.fk_cliente inner join items on pedidos.ped_id = items.fk_pedido inner join produtos on
   produtos.pro_id = items.fk_produto where clientes.cli_id = 2 AND pedidos.ped_id = 4]]>
	</queryString>
